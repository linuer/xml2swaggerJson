<?xml version="1.0" ?>
<Resources>
	<Resource name="JournalEntries" type="Entity">
		<Summary>This entity enables you to manipulate 'JournalEntries'. It represents journal transactions.</Summary>
		<Functions>
			<Function method="get" resource_path="JournalEntries(id)">
				<Description>Retrieve all or some selected properties from an instance of 'JournalEntries' with the given id.</Description>
				<Example>
					<Case url="https://slcdemo.cfapps.sap.hana.ondemand.com/b1s/v1/JournalEntries(123)"/>
					<Case url="https://slcdemo.cfapps.sap.hana.ondemand.com/b1s/v1/JournalEntries(123)?$select=ReferenceDate,Memo,Reference"/>
				</Example>
			</Function>
			<Function method="get" resource_path="JournalEntries">
				<Description>Retrieve a collection of 'JournalEntries' with all or some selected properties in the given order by specifying the given filter condition.</Description>
				<Example>
					<Case url="https://slcdemo.cfapps.sap.hana.ondemand.com/b1s/v1/JournalEntries"/>
					<Case url="https://slcdemo.cfapps.sap.hana.ondemand.com/b1s/v1/JournalEntries?$select=ReferenceDate,Memo,Reference&amp;$filter=JdtNum ge 123 &amp;$orderby=JdtNum&amp;$top=10&amp;$skip=1"/>
				</Example>
			</Function>
			<Function method="post" resource_path="JournalEntries">
				<Description>Create an instance of 'JournalEntries' with the given payload of type 'JournalEntry' in JSON format.</Description>
				<Example>
					<Case url="https://slcdemo.cfapps.sap.hana.ondemand.com/b1s/v1/JournalEntries">
					{
						"JournalEntryLines": [{
							"AccountCode": "_SYS00000000019",
							"Credit": 100,
							"Debit": 0
						},
						{
							"AccountCode": "_SYS00000000164",
							"Credit": 0,
							"Debit": 100
						}]
					}
					</Case>
				</Example>
			</Function>
			<Function method="patch" resource_path="JournalEntries(id)">
				<Description>Update an instance of 'JournalEntries' with the given payload of type 'JournalEntry' in JSON format and with the specified id.</Description>
				<Example>
					<Case url="https://slcdemo.cfapps.sap.hana.ondemand.com/b1s/v1/JournalEntries(123)">
					{
						"Memo": "new memo - modified by Service Layer"
					}
					</Case>
				</Example>
			</Function>
			<Function method="post" resource_path="JournalEntries(id)/Cancel">
				<Description>Invoke the method 'Cancel' on this EntitySet with the specified id.</Description>
				<Example>
					<Case url="https://slcdemo.cfapps.sap.hana.ondemand.com/b1s/v1/JournalEntries(123)/Cancel"/>
				</Example>
			</Function>
		</Functions>
