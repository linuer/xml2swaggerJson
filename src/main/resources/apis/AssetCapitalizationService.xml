<?xml version="1.0" ?>
<Resources>
	<Resource name="AssetCapitalizationService" type="Service">
		<Summary>This API enables you to invoke the interfaces defined on 'AssetCapitalizationService'.</Summary>
		<Functions>
			<Function method="post" resource_path="AssetCapitalizationService_Cancel">
				<Description>Invoke the method 'Cancel' on this service by specifying the payload 'AssetDocumentParams' in the JSON format.</Description>
				<Example>
					<Case url="https://slcdemo.cfapps.sap.hana.ondemand.com/b1s/v1/AssetCapitalizationService_Cancel">
					{
					"AssetDocumentParams":
						{
						"Code":"1",
						"CancellationOption" :"coByCurrentSystemDate"
						}
					}</Case>
				</Example>
			</Function>
			<Function method="post" resource_path="AssetCapitalizationService_GetList">
				<Description>Invoke the method 'GetList' on this service.</Description>
				<Example>
					<Case url="https://slcdemo.cfapps.sap.hana.ondemand.com/b1s/v1/AssetCapitalizationService_GetList"/>
				</Example>
			</Function>
		</Functions>
		<DataTypes>
			<Description>The 'DataTypes' defines the types referenced by the 'Functions'. It is only for QA, not for info dev.</Description>
			<ComplexType name="AssetDocumentParams">
				<Property Name="Code" Type="integer"/>
				<Property Name="CancellationOption" Type="ClosingOptionEnum"/>
				<Property Name="CancellationDate" Type="string"/>
			</ComplexType>
			<EnumType Name="ClosingOptionEnum">
				<Member Name="coBySpecifiedDate"/>
				<Member Name="coByCurrentSystemDate"/>
				<Member Name="coByOriginalDocumentDate"/>
			</EnumType>
		</DataTypes>
	</Resource>
</Resources>
